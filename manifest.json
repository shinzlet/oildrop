{
	"manifest_version": 2,
	"name": "Oildrop",
	"version": "0.0.1",
	"description": "An open-source userscript manager that you can audit on your own.",
	"permissions": [
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_title": "Oildrop",
		"default_popup": "ui.html"
	},
	"background": {
		"scripts": ["oildrop.js", "background.js"]
	}
}
